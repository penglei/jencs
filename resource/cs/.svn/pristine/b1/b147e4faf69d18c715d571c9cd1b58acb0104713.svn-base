WUPMAIN_LIB = /usr/local/qzone_v3.0/lib/libwupmain.cs.so
WUPMAIN_V8_LIB = /usr/local/qzone_v3.0/lib/libwupmain_v8.cs.so

all:
	./trim_dir.sh 
	cd ../;tools/extinclude ../wupmain.cs tools/wupmain.cs;cd -
	./gencpp wupmain.cs
	cd ../;tools/extinclude ../wupmain_v8.cs tools/wupmain_v8.cs;cd -
	./gencpp wupmain_v8.cs
	g++ -Icsinclude -Wall -shared -o  $(WUPMAIN_LIB) wupmain.cs.cpp
	g++ -Icsinclude -Wall -shared -o  $(WUPMAIN_V8_LIB) wupmain_v8.cs.cpp

clean:
	rm wupmain_v8.cs wupmain_v8.cs.h wupmain_v8.cs.cpp wupmain.cs wupmain.cs.h wupmain.cs.cpp
